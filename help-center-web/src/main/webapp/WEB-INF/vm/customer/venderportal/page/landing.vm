    $(function(){
        seajs.use('jdf/2.0.0/ui/gotop/1.0.0/gotop',function(gotop){
            var gotopBtn = $('.gotop');
            gotopBtn.gotop({
                btnEl: '#gotop',
                isSyncShow: false,
                easing: 'swing'
            });
        });

    });
</script>
